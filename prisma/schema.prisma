generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id       String @id @default(uuid())
  name     String
  email    String
  password String
}

model response {
  id           String @id @default(uuid())
  interview_id Int
  question_id  String
  note         Int
  obs          String
}

model interview {
  id Int @id @default(autoincrement())
}

model question {
  id      String @id @default(uuid())
  content String
}
